<Window x:Class="Arae.AddTagWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add Tag" Height="300" Width="202" Loaded="Window_Loaded">
    <Grid>
        <Label Content="{Binding SelectedPath}" Height="25" HorizontalAlignment="Stretch" Margin="12,12,12,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="Add New Tag" Height="25" HorizontalAlignment="Left" Margin="12,79,0,0" Name="button1" VerticalAlignment="Top" Width="120" />
        <TextBox Height="25" HorizontalAlignment="Left" Margin="12,48,0,0" Name="textBlock1" Text="Enter New Tag" VerticalAlignment="Top" Width="156" GotFocus="textBlock1_GotFocus" />
        <ListBox Height="100" HorizontalAlignment="Left" Margin="12,118,0,0" Name="listBox1" VerticalAlignment="Top" Width="156" />
        <Button Content="Add Existing Tags" Height="25" HorizontalAlignment="Left" Margin="12,224,0,0" Name="button2" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
